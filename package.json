{
  "name": "rest-api-aws-cdk",
  "version": "0.1.0",
  "bin": {
    "rest-api-aws-cdk": "bin/rest-api-aws-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth:dev": "cdk synth --context stage=dev --profile dixon",
    "bootstrap:dev": "cdk bootstrap --context stage=dev --profile dixon",
    "deploy:dev": "cdk deploy --context stage=dev --profile dixon --require-approval never",
    "destroy:dev": "cdk destroy --context stage=dev --profile dixon",
    "start": "sam local start-api -t ./cdk.out/RestApiAwsCdkStack-dev.template.json"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1020.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.202.0",
    "constructs": "^10.0.0"
  }
}
